{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --ignore-path .gitignore",
      "git add"
    ],
    "*.php": [
      "php -l"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "blueimp-canvas-to-blob": "^3.14.0",
    "blueimp-load-image": "^2.17.1",
    "element-ui": "^2.0.7",
    "flatpickr": "^3.1.5",
    "hamburgers": "^0.9.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.0",
    "mint-ui": "^2.2.12",
    "nice-validator": "^1.1.3",
    "promise.prototype.finally": "^3.1.0",
    "quill": "^1.3.4",
    "spark-md5": "^3.0.0",
    "store": "^2.0.4",
    "tailwindcss": "^0.3.0",
    "tinymce": "^4.7.4",
    "videojs-offset": "^2.0.0-beta.2",
    "videojs-vjsdownload": "^1.0.3",
    "viewerjs": "^0.10.0",
    "vue": "^2.5.9",
    "vue-router": "^3.0.1",
    "vue-video-player": "^5.0.2",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-plugin-component": "^0.10.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.13.0",
    "eslint-config-google": "^0.9.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-vue": "^3.14.0",
    "husky": "^0.14.3",
    "laravel-mix": "^1.7.2",
    "lint-staged": "^6.0.0",
    "vue-eslint-parser": "^2.0.1-beta",
    "vue-template-compiler": "^2.5.9"
  },
  "engines": {
    "node": ">=7"
  },
  "browserslist": [
    "Firefox >= 20",
    "Chrome >= 30",
    "ie >= 9",
    "Safari >= 6"
  ]
}
